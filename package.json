{
  "name": "prompt-compose",
  "version": "0.0.3",
  "description": "A JS library for building and managing GPT prompts documents.",
  "main": "./dist/main.mjs",
  "module": "./dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist",
    "src",
    "test"
  ],
  "keywords": [
    "chatGPT",
    "GPT",
    "prompt",
    "prompt-builder",
    "builder",
    "document",
    "prompt-document",
    "manager",
    "nodejs",
    "AI",
    "library",
    "prompt maker",
    "maker"
  ],
  "scripts": {
    "build": "parcel build",
    "build-test": "parcel build --target tests",
    "watch": "parcel watch",
    "test": "npm run build-test && node ./dist/tests/index.js",
    "prepare": "npm run test && npm run build",
    "check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anubhavgupta/prompt-compose.git"
  },
  "homepage": "https://github.com/anubhavgupta/prompt-compose",
  "targets": {
    "main": {
      "optimize": true,
      "outputFormat": "esmodule",
      "engines": {
        "node": ">= 16"
      },
      "source": "./src/index.ts"
    },
    "module": {
      "optimize": true,
      "outputFormat": "esmodule",
      "engines": {
        "browsers": "Chrome 80"
      },
      "source": "./src/index.ts"
    },
    "tests": {
      "optimize": false,
      "outputFormat": "global",
      "engines": {
        "node": ">= 16"
      },
      "source": "./tests/index.ts"
    }
  },
  "author": "anubhav200@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@parcel/packager-ts": "^2.9.3",
    "@parcel/transformer-typescript-types": "^2.9.3",
    "@types/node": "^20.5.1",
    "parcel": "^2.9.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16"
  }
}
